<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ai.aichatbackend.mapper.PromptsMapper">

    <select id="selectList" resultType="com.ai.aichatbackend.domain.Prompts">
        select * from prompts;
    </select>

    <select id="selectByType" resultType="com.ai.aichatbackend.domain.Prompts">
        select * from PROMPTS
        where PROMPT_TYPE = #{type}
    </select>
</mapper>